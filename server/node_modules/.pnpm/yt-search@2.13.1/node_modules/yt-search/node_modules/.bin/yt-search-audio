#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules/yt-search/bin/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules/yt-search/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules/yt-search/bin/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules/yt-search/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/yt-search@2.13.1/node_modules:/home/jefferson/Documentos/projetos/OtaTube/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir//bin/bash" ]; then
  exec "$basedir//bin/bash"  "$basedir/../../bin/mpv_audio.sh" "$@"
else
  exec /bin/bash  "$basedir/../../bin/mpv_audio.sh" "$@"
fi
